ault:\n      return state;\n  }\n};\n\nexport default reducer;\n","/Users/johnmarsden/Desktop/burgerbuilder/src/store/reducers/order.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/actions/actionTypes.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/actions/index.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/actions/burgerBuilder.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/actions/order.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/utility.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/containers/Auth/Auth.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/actions/auth.js",[],"/Users/johnmarsden/Desktop/burgerbuilder/src/store/reducers/auth.js",[],{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","replacedBy":"168"},{"ruleId":"169","severity":1,"message":"170","line":7,"column":13,"nodeType":"171","messageId":"172","endLine":7,"endColumn":20},{"ruleId":"169","severity":1,"message":"173","line":1,"column":10,"nodeType":"171","messageId":"172","endLine":1,"endColumn":13},"no-native-reassign",["174"],"no-negated-in-lhs",["175"],"no-unused-vars","'actions' is defined but never used.","Identifier","unusedVar","'act' is defined but never used.","no-global-assign","no-unsafe-negation"]